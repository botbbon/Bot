### Copy this file to `.env` and fill real API keys before running live tests.
### DO NOT commit your real .env with secrets to git.

# Helius (preferred for on-chain signatures / parse history)
HELIUS_API_KEYS=your_helius_key_1,your_helius_key_2
# fallback single key (optional if HELIUS_API_KEYS provided)
HELIUS_API_KEY=
HELIUS_RPC_URL=https://rpc.helius.example
HELIUS_PARSE_HISTORY_URL=
HELIUS_WS_URL_RAW=

# Solscan (optional fallback / explorer)
SOLSCAN_API_URL=https://public-api.solscan.io
SOLSCAN_API_KEYS=your_solscan_key_1,your_solscan_key_2

# Jupiter (quote API)
JUPITER_QUOTE_API=https://quote-api.jup.ag/v6/quote
JUPITER_API_KEYS=your_jupiter_key_1,your_jupiter_key_2

# RPC (fallback if you don't have Helius RPC)
MAINNET_RPC=https://api.mainnet-beta.solana.com

# Limits for the quick live test
CAND_LIMIT=30
HELIUS_VALIDATE_CONCURRENCY=4
FIND_FRESH_TIMEOUT_MS=8000

# Other tuneables (optional)
ONCHAIN_CONCURRENCY=2
HELIUS_ENRICH_LIMIT=8
